<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.openmrs.module.ctechdev">
    <class name="CtechDeveloper" table="ctech_devs" lazy="false">
        <id name="id" column="ctech_dev_id">
            <generator class="native"/>
        </id>
        <property name="jobId" type="java.lang.String" column="job_id" length="38"/>
        <property name="name" type="java.lang.String" column="name" length="255"/>
        <property name="gender" type="java.lang.String" column="gender" length="10"/>
        <property name="designation" type="java.lang.String" column="designation" length="255"/>
    </class>
</hibernate-mapping>
